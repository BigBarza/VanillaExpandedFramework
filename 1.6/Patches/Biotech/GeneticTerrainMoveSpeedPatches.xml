<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>Biotech</li>
      </mods>
      <!-- Insert right after Vanilla's StatPart_TerrainMoveSpeed. -->
      <match Class="PatchOperationInsert">
         <xpath>Defs/StatDef/parts/li[@Class="StatPart_TerrainMoveSpeed"]</xpath>
         <order>Append</order>
         <value>
            <li Class="VEF.Genes.StatPart_GeneticTerrainMoveSpeed" />
         </value>
      </match>
   </Operation>
</Patch>
